<div class="profile-form-wrapper">
  <%= form_for [group, event] do |f|  %>
  <%= f.label :name %>
  <%= f.text_area :name %><br />
  <%= f.label :event_start %>
  <%= f.date_field :event_start %><br />
  <%= f.label :event_end %>
  <%= f.date_field :event_end %><br />
  <%= f.label :description %>
  <%= f.text_area :description %><br />
  <%= f.submit "Add Event", class: "btn theme-btn-1 update-prof-btn" %>
  <% end %>
</div>
